<?xml version="1.0"?>
<terp>
<data>

	#
	# Tasks by projects and users
	#

	<record model="ir.ui.view" id="view_commission_month_form">
		<field name="name">commission.month.form</field>
		<field name="model">ommission.month</field>
		<field name="type">form</field>
		<field name="arch" type="xml">
			<form string="sales agent commission">
				<field name="name" select="1"/>
				<field name="SoNo" select="1"/>
				<field name="InvNo" select="1"/>
				<field name="InvTotal" select="1"/>
				<field name="InvDate" select="1"/>
				<field name="comrate" select="1"/>
				<field name="commission" select="1"/>
				<field name="state"/>
			</form>
		</field>
	</record>


	<record model="ir.ui.view" id="view_commission_month_tree">
		<field name="name">commission.month.form.tree</field>
		<field name="model">commission.month</field>
		<field name="type">tree</field>
		<field name="arch" type="xml">
			<tree string="sales agent commission">
				<field name="name" select="1"/>
				<field name="SoNo" select="1"/>
				<field name="InvNo" select="1"/>
				<field name="InvTotal" select="1"/>
				<field name="InvDate" select="1"/>
				<field name="comrate" select="1"/>
				<field name="commission" select="1"/>
				<field name="state"/>
			</tree>
		</field>
	</record>
	<record model="ir.actions.act_window" id="action_sales agent commission_tree_month">
		<field name="name">commission.month.form.tree</field>
		<field name="res_model">commission.month</field>
		<field name="view_type">form</field>
		<field name="view_mode">tree</field>
		<!--field name="domain">[('name','=',time.strftime('%Y-%m-01'))]</field-->
	</record>
<menuitem name="Sales Management/Commissions/Reporting/This Month/Commissions of This Month" action="action_sales agent commission_tree_month" id="menu_commission_tree_month"/>
</data>
</terp>
